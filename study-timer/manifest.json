{
    "manifest_version": 3,
    "name": "Study timer",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "background": {
        "service_worker": "src/background.ts",
        "type": "module"
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ]
}